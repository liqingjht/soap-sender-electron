  "author": "liqingjht",
  "email": "liqingjht@163.com",
  "homepage": "http://127.0.0.1:7168",
  "description": "A minimal soap sender desktop app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pk": "electron-packager ./ --electron-version=1.8.4 --out=../release --icon=./assets/logo.ico --asar --overwrite --download.mirror=https://npm.taobao.org/mirrors/electron/ ",
    "pk-linux": "electron-packager ./ --electron-version=1.8.4 --out=../release --icon=./assets/logo.ico --asar --overwrite --platform=linux --arch=x64,ia32 --download.mirror=https://npm.taobao.org/mirrors/electron/",
    "pk-win": "electron-packager ./ --electron-version=1.8.4 --out=../release --icon=./assets/logo.ico --asar --overwrite --platform=win32 --arch=x64,ia32 --download.mirror=https://npm.taobao.org/mirrors/electron/",
    "pk-mac": "electron-packager ./ --electron-version=1.8.4 --out=../release --icon=./assets/logo.icns --asar --overwrite --platform=darwin --arch=x64 --download.mirror=https://npm.taobao.org/mirrors/electron/"
  },
  "keywords": [
    "Electron",
    "SOAP"
  ],
  "license": "MPL",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^11.2.0"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "clipboard": "^2.0.0",
    "request": "^2.83.0",
    "xml-formatter": "^1.0.1"
  }
